<?xml version="1.0"?>
<extension key="cards.iwwa.newsletter" type="module">
  <file>newsletter</file>
  <name>CivIWWA newsletter</name>
  <description>Creates the CiviCRM group for the newsletter,
  and links it to Mailchimp.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Johan Vervloet</author>
    <email>johanv@johanv.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/johanv/civiwwa</url>
    <url desc="Documentation">https://github.com/johanv/civiwwa</url>
    <url desc="Support">https://github.com/johanv/civiwwa</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-01-03</releaseDate>
  <version>1.0</version>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>Enables the newsletter synchronisation.</comments>
  <civix>
    <namespace>CRM/Newsletter</namespace>
  </civix>
  <requires>
    <ext>uk.co.vedaconsulting.mailchimp</ext>
  </requires>
</extension>
